{{> weather}}
<div>
    {{#each articles}}
    <article class='news'>
        <div>
            <div>{{ title }}</div>
        </div>
        <div>
            {{#if urlToImage}}
                <img  src="{{urlToImage}}" width="100" height="auto">
            {{/if}}
            <div>{{ description }}</div>
        </div>
        <div>
            <a href="{{ url }}">{{ source.name }}</a>
        </div>
    </article>
    {{/each}}
</div>
